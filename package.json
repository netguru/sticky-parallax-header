{
  "name": "react-native-sticky-parallax-header",
  "version": "0.4.2",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "assets",
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-sticky-parallax-header.podspec",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "repository": "https://github.com/netguru/sticky-parallax-header",
  "author": "IdaszakDaniel <idaszak1@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "echo pods",
    "bootstrap": "yarn example && yarn && yarn pods",
    "prepush": "yarn lint && yarn typescript && yarn test"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^3.3.0",
    "@testing-library/react-native": "^7.0.2",
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.62.13",
    "@types/react-test-renderer": "^17.0.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.2.3",
    "jest": "^26.4.2",
    "prettier": "^2.3.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-builder-bob": "^0.18.0",
    "react-test-render": "^1.1.2",
    "react-test-renderer": "^16.13.1",
    "release-it": "^14.11.5",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prepush",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release-it": {
    "hooks": {
      "after:release": "git checkout -b releases/v_${version} && git push -u origin releases/v_${version}"
    },
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}",
      "push": false
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  }
}
