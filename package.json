{
  "name": "react-native-sticky-parallax-header",
  "version": "0.4.1",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "source": "src/index",
  "repository": "https://github.com/netguru/sticky-parallax-header",
  "author": "IdaszakDaniel <idaszak1@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "prepush": "yarn test && yarn lint",
    "release": "release-it",
    "test": "jest"
  },
  "dependencies": {
    "prop-types": "^15.7.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "^3.3.0",
    "@testing-library/react-native": "^7.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-reactnative": "^1.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "eslint-plugin-react-native": "^3.9.1",
    "husky": "^4.2.3",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-test-renderer": "^16.13.1",
    "release-it": "^14.11.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn prepush",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "release-it": {
    "hooks": {
      "after:release": "git checkout -b releases/v_${version} && git push -u origin releases/v_${version}"
    },
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}",
      "push": false
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  }
}
